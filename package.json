{
  "name": "next-boilerplate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cp .env.local .env && next dev",
    "dev:staging": "cp ./envs/.env.staging .env.local && next dev",
    "dev:prod": "cp ./envs/.env.production .env.local && next dev",
    "db:push": "npx prisma db push",
    "db:generate": "npx prisma generate",
    "db:migrate": "npx prisma migrate dev --name dev",
    "db:migrate:prod": "npx prisma migrate deploy --schema=./prisma/schema.prisma",
    "db:migrate:staging": "npx prisma migrate deploy --schema=./prisma/schema.prisma",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky",
    "commit": "git-cz",
    "lint:fix": "next lint --fix --ignore-path .eslintignore",
    "lint:css": "prettier '**/*.{json,yaml,css,scss}' --write --ignore-path .eslintignore",
    "format": "yarn lint:fix && yarn lint:css"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "dependencies": {
    "@ant-design/cssinjs": "^1.21.1",
    "@ant-design/nextjs-registry": "^1.0.1",
    "@auth/mongodb-adapter": "^3.4.2",
    "@auth/prisma-adapter": "^2.4.2",
    "@ducanh2912/next-pwa": "^10.2.8",
    "@formatjs/intl-localematcher": "^0.5.4",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@next/bundle-analyzer": "^14.2.11",
    "@prisma/client": "^5.19.1",
    "@reduxjs/toolkit": "^2.2.7",
    "@svgr/webpack": "^8.1.0",
    "@swc/cli": "^0.4.1-nightly.20240914",
    "@swc/core": "^1.7.26",
    "@tanstack/react-query-devtools": "^5.56.2",
    "antd": "^5.20.6",
    "aws4": "^1.13.2",
    "bcryptjs": "^2.4.3",
    "classnames": "^2.5.1",
    "dayjs": "^1.11.13",
    "lodash": "^4.17.21",
    "mongodb": "^6.9.0",
    "negotiator": "^0.6.3",
    "next": "14.2.11",
    "next-auth": "^4.24.7",
    "next-compose-plugins": "^2.2.1",
    "next-intl": "^3.20.0",
    "next-plugin-svgr": "^1.1.10",
    "postcss-antd-fixes": "^0.2.0",
    "prisma": "^5.19.1",
    "react": "^18",
    "react-dom": "^18",
    "react-icons": "^5.3.0",
    "react-query": "^3.39.3",
    "react-redux": "^9.1.2",
    "react-use-websocket": "^4.8.1",
    "recharts": "^2.12.7",
    "redux-deep-persist": "^1.0.7",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0",
    "sharp": "^0.33.5",
    "socket.io-client": "^4.7.5",
    "zustand": "^5.0.0-rc.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/negotiator": "^0.6.3",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-countup": "^4.3.1",
    "@types/react-dom": "^18",
    "@types/recharts": "^1.8.29",
    "@types/redux-logger": "^3.0.13",
    "autoprefixer": "^10.4.20",
    "eslint": "^8",
    "eslint-config-next": "14.2.11",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.36.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "path": "^0.12.7",
    "postcss": "^8",
    "prettier": "^3.3.3",
    "sass": "^1.78.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
